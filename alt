				print('sinuca')
				tela_jogo = True
				b_sinuca = False


			elif b_redbull:

				for event in pygame.event.get():
					# Captura evento QUIT que fexa o programa no X
					if event.type == pygame.QUIT:
						pygame.quit()  # Fexa o programa ao paertar no X
						sys.exit()  # Finaliza processos ao fexar o programa

					# Captura evento do mouse
					if event.type == pygame.MOUSEBUTTONDOWN:
						print(pygame.mouse.get_pos())                        
						
					if tm < 1:
						tm += 1
						pygame.time.wait(1000)

					if event.type == pygame.KEYDOWN:

						if event.key == pygame.K_w:
							print(self.bonus(10))
							print('escolheu 1')
							tm = 0
							tela_jogo = True
							b_redbull = False

				frame.tick(30)
				tela.blit(pygame.transform.scale(bonus_garrafa, scala), (0, 0))
				pygame.display.update()

			elif b_bruxa:
				g = False
				lop += 60
				crd = font.render(str(credito), 1, cor_amarela)
				roleta_garrafa = pygame.image.load(local + 'garrafa.png')
				
				lock = (382, 318)
				new_roleta_garrafa = pygame.transform.rotate(roleta_garrafa, lop)
				rot = new_roleta_garrafa.get_rect(center = lock)


				for event in pygame.event.get():
					# Captura evento QUIT que fexa o programa no X
					if event.type == pygame.QUIT:
						pygame.quit()  # Fexa o programa ao paertar no X
						sys.exit()  # Finaliza processos ao fexar o programa

					# Captura evento do mouse
					if event.type == pygame.MOUSEBUTTONDOWN:
						print(pygame.mouse.get_pos())                        
						
					if tm < 1:
						tm += 1
						pygame.time.wait(1000)

					if event.type == pygame.KEYDOWN:
						if event.key == pygame.K_q:
							pygame.quit()
							sys.exit()

						if event.key == pygame.K_g:
							if valor_aposta <= 100:
								escolha = random.randint(1, 27)
							elif valor_aposta <= 500:
								escolha = random.randint(1, 10)
							elif valor_aposta >= 500:
								escolha = random.randint(1, 5)

							premio = valor_aposta * escolha
							credito += premio
							pago += premio

							new_roleta_garrafa = pygame.transform.rotate(roleta_garrafa, (6.66 * escolha))

							tela.blit(new_roleta_garrafa, rot)

							print(escolha)
							tm = 0
							g = True

							txt_credito = font.render(str(credito), 1, cor_amarela)
							txt_premio = font.render(str(premio), 1, cor_amarela)
							txt_pago = font_pago.render(str(pago), 1, cor_amarela)


				frame.tick(30)
				tela.blit(pygame.transform.scale(bonus_roleta, scala), (0, 0))
				tela.blit(new_roleta_garrafa, rot)

				tela.blit(crd, (332, 560))
				tela.blit(txt_premio, (709, 560))
				pygame.display.update()
				if g:
					pygame.time.wait(5000)
					b_bruxa = False
					tela_jogo = True

			elif tela_pagamento:
				pass



			elif tela_config:

				for event in pygame.event.get():
					cor_amarela = (196, 196, 196)
					txt_cfg = font.render(str('tela de config'), 1, cor_amarela)
					if event.type == pygame.KEYDOWN:
						if event.key == pygame.K_j:
							tela_jogo = True
							tela_config = False

					frame.tick(30)
					tela.blit(pygame.transform.scale(bonus_garrafa, scala), (0, 0))  # Deixa Scalavel

					tela.blit(txt_cfg, (0,0))

					# Ativa SCLE de tela
					pygame.display.flip()

					# Serve para atualizar a tela a cada loop
					pygame.display.update()


	#define em qual linha o ojador quer
	def linha(self, tipo):
		if tipo == 1:
			return 1
		if tipo == 5:
			return 5
		if tipo == 9:
			return 9
		if tipo == 15:
			return 15
		if tipo == 25:
			return 25

	def bonus(self, val):
		RETEN = [500, 150, 100, 75, 50, 25]
		for i in range(self.retencao):
			RETEN.append(100)
			RETEN.append(75)
			RETEN.append(50)
			RETEN.append(25)

		return random.choice(RETEN)



	def roleta(self, aposta, linhas, apValor):
		data = self.banco
		var = "SELECT reten FROM user WHERE usuario = 'admin'"
		ex = data.cursor()
		ex.execute(var)

		#Pega Valor do Banco par informar o valor a reter
		RET = ex.fetchall()[0][0]
		self.retencao = RET

		#referencia da imagens da roleta
		imagens = {'01': '01.jpg', '02': '02.jpg', '03': '03.jpg', '04': '04.jpg', '05': '05.jpg', '06': '06.jpg', '07': '07.jpg',
										   '08': '08.jpg', '09': '09.jpg', '10': '10.jpg' }

		#Lista com valores para sorteio quanto mais numero maior a retenção
		retencao = ['01', '02', '03', '04', '05', '06', '07', '08', '09', '10']

		if RET > 1:
			for add in range(int(RET)):

				retencao.append('05')
				retencao.append('03')
				retencao.append('05')
				retencao.append('04')
				retencao.append('05')

				retencao.append('08')
				retencao.append('06')				
				retencao.append('08')
				retencao.append('07')
				retencao.append('08')

				retencao.append('09')
				retencao.append('10')
				retencao.append('09')
				retencao.append('09')

		img = []

		listagen = {}
		for i in range(1, 16):

			rand = imagens[random.choice(retencao)]
			img.append(rand)
			r = 'l'+str(i)
			listagen[r] = rand

		verifica_premios = ''

		if linhas == 25:
			verifica_premios = self.linha25(listagen, apValor)
			self.pr_valor = verifica_premios[3]

		if linhas == 15:
			verifica_premios = self.linha15(listagen, apValor)
			self.pr_valor = verifica_premios[3]

		if linhas == 9:
			verifica_premios = self.linha9(listagen, apValor)
			self.pr_valor = verifica_premios[3]

		if linhas == 5:
			verifica_premios = self.linha5(listagen, apValor)
			self.pr_valor = verifica_premios[3]

		if linhas == 1:
			verifica_premios = self.linha1(listagen, apValor)
			self.pr_valor = verifica_premios[3]

		return img, verifica_premios

	def salva(self, ap, pr, pa, cr, va):
		data = self.banco
		relogio = str(datetime.datetime.now()).split('.')[0]
		hora = relogio
		# Salva ultimo estado do credito da maquina
		#if tipo == 'credito':
		var = "INSERT INTO ultima_aposta (id, B_aposta, B_premio, B_pago, B_credito, B_valor_aposta, data) VALUES (NULL, '%s', '%s', '%s', '%s', '%s', '%s');" % (ap, pr, pa, cr, va, hora)
		grava = data.cursor()
		grava.execute(var)
		data.commit()


	def linha25(self, rolos, val):
		imagens = {'01.jpg': 500,'02.jpg': 300, '03.jpg': 25, '04.jpg': 20, '05.jpg': 9, '06.jpg': 7, '07.jpg': 5,
										   '08.jpg': 4, '09.jpg': 2, '10.jpg': 1 }

		valor_ganho = (15 * val / 100)
		bonus = ''
		wins = {}
		img_wins = {}
		#Premio vertical por coluna
		if rolos['l1'] == rolos['l2']:
			if rolos['l1'] == rolos['l3']:
				img_wins['l1'] = rolos['l1']
				valor_ganho += 3 * imagens[rolos['l1']]
				wins['l1'] = valor_ganho
				if rolos['l1'] == '09.jpg':
					bonus = 'bruxa'
				elif rolos['l1'] == '08.jpg':
					bonus = 'sinuca'
				elif rolos['l1'] == '05.jpg':
					bonus = 'redbull'

		if rolos['l4'] == rolos['l5']:
			if rolos['l4'] == rolos['l6']:
				img_wins['l4'] = rolos['l6']
				valor_ganho += 3 * imagens[rolos['l4']]
				wins['l4'] = valor_ganho
				if rolos['l4'] == '09.jpg':
					bonus = 'bruxa'
				elif rolos['l4'] == '08.jpg':
					bonus = 'sinuca'
				elif rolos['l4'] == '05.jpg':
					bonus = 'redbull'

		if rolos['l7'] == rolos['l8']:
			if rolos['l7'] == rolos['l9']:
				img_wins['l7'] = rolos['l7']
				valor_ganho += 3 * imagens[rolos['l7']]
				wins['l7'] = valor_ganho
				if rolos['l7'] == '09.jpg':
					bonus = 'bruxa'
				elif rolos['l7'] == '08.jpg':
					bonus = 'sinuca'
				elif rolos['l7'] == '05.jpg':
					bonus = 'redbull'

		if rolos['l10'] == rolos['l11']:
			if rolos['l10'] == rolos['l12']:
				img_wins['l10'] = rolos['l10']
				valor_ganho += 3 * imagens[rolos['l10']]
				wins['l10'] = valor_ganho
				if rolos['l10'] == '09.jpg':
					bonus = 'bruxa'
				elif rolos['l10'] == '08.jpg':
					bonus = 'sinuca'
				elif rolos['l10'] == '05.jpg':
					bonus = 'redbull'

		if rolos['l13'] == rolos['l14']:
			if rolos['l13'] == rolos['l15']:
				img_wins['l15'] = rolos['l13']
				valor_ganho += 3 * imagens[rolos['l13']]
				wins['l15'] = valor_ganho
				if rolos['l13'] == '09.jpg':
					bonus = 'bruxa'
				elif rolos['l13'] == '08.jpg':
					bonus = 'sinuca'
				elif rolos['l13'] == '05.jpg':
					bonus = 'redbull'

		#Premio horizontal por linha
		if rolos['l1'] == rolos['l4']:
			if rolos['l1'] == rolos['l7']:
				if rolos['l1'] == rolos['l10']:
					if rolos['l1'] == rolos['l13']:
						img_wins['l65'] = rolos['l1']
						valor_ganho += 3 * imagens[rolos['l1']]
						wins['l65'] = valor_ganho
					else:
						img_wins['l64'] = rolos['l1']
						valor_ganho += 3 * imagens[rolos['l1']]
						wins['l64'] = valor_ganho
				else:
					img_wins['l63'] = rolos['l1']
					valor_ganho += 3 * imagens[rolos['l1']]
					wins['l63'] = valor_ganho

		if rolos['l7'] == rolos['l10']:
			if rolos['l7'] == rolos['l13']:
				if rolos['l7'] == rolos['l4']:
					if rolos['l7'] == rolos['l1']:
						img_wins['l75'] = rolos['l7']
						valor_ganho += 3 * imagens[rolos['l7']]
						wins['l75'] = valor_ganho
					else:
						img_wins['l74'] = rolos['l7']
						valor_ganho += 3 * imagens[rolos['l7']]
						wins['l74'] = valor_ganho
				else:
					img_wins['l73'] = rolos['l7']
					valor_ganho += 3 * imagens[rolos['l7']]
					wins['l73'] = valor_ganho

		if rolos['l2'] == rolos['l5']:
			if rolos['l2'] == rolos['l8']:
				if rolos['l2'] == rolos['l11']:
					if rolos['l2'] == rolos['l14']:
						img_wins['l225'] = rolos['l2']
						valor_ganho += 3 * imagens[rolos['l2']]
						wins['l225'] = valor_ganho
					else:
						img_wins['l224'] = rolos['l2']
						valor_ganho += 3 * imagens[rolos['l2']]
						wins['l224'] = valor_ganho
				else:
					img_wins['l23'] = rolos['l2']
					valor_ganho += 3 * imagens[rolos['l2']]
					wins['l23'] = valor_ganho

		if rolos['l8'] == rolos['l11']:
			if rolos['l8'] == rolos['l14']:
				if rolos['l8'] == rolos['l5']:
					if rolos['l8'] == rolos['l2']:
						img_wins['l85'] = rolos['l8']
						valor_ganho += 3 * imagens[rolos['l8']]
						wins['l85'] = valor_ganho
					else:
						img_wins['l84'] = rolos['l8']
						valor_ganho += 3 * imagens[rolos['l8']]
						wins['l84'] = valor_ganho
				else:
					img_wins['l83'] = rolos['l8']
					valor_ganho += 3 * imagens[rolos['l8']]
					wins['l83'] = valor_ganho


		if rolos['l3'] == rolos['l6']:
