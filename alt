		valor_ganho = 0
		bonus = ''
		wins = {}
		img_wins = {}

		if rolos['l2'] == rolos['l5']:
			if rolos['l2'] == rolos['l8']:
				if rolos['l2'] == rolos['l11']:
					if rolos['l2'] == rolos['l14']:
