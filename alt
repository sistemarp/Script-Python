	def deposito(self, vlr):
		data = self.banco
		relogio = str(datetime.datetime.now()).split('.')[0]
		hora = relogio
		var = "INSERT INTO deposito (id, dp, data) VALUES (DEFAULT, '%s', '%s');" % (vlr, hora)
		grava = data.cursor()
		grava.execute(var)
		data.commit()

if __name__ == '__main__':
	Jogo().main()
