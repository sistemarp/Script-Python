import pygame, sys, os, MySQLdb, datetime, random
from pygame.locals import *


class Banco(object):
	def dados(self):
		try:
			conecta = MySQLdb.connect(host='localhost', user='root', passwd='toor', db='mmatriz')
			return conecta
		except Exception as e:
			print('Deu erro no Banco de Dados --> ', str(e))


class Jogo(pygame.sprite.Sprite):
	banco = Banco().dados()
	retencao = 0
	pr_valor = ''

	def main(self):
		scala = (1360, 720)
		pygame.init()
		tela = pygame.display.set_mode(scala)#adicione pygame.FULLSCREEN para deixar full scree
		pygame.display.set_caption('Anderson Padovani Sistemas')
		frame = pygame.time.Clock()

		local = os.getcwd() + os.sep + '_imagens' + os.sep

		back_ground = pygame.image.load(local + 'sob-fundo.png')
		bonus_garrafa = pygame.image.load(local + 'bonus_garrafa.jpg')
		bonus_roleta = pygame.image.load(local + 'bonus_roleta.jpg')

		# Cria um estilo de fonte padrão para ser exibido na tela
		style = pygame.font.get_default_font()
		font = pygame.font.SysFont(style, 65)
		font_pago = pygame.font.SysFont(style, 30)

		credito = 0
		premio = 0
		aposta = 1
		pago = 0
		valor_aposta = 1
		linha = 1
		divi = ''

		premiacao = {}

		#cores para letras e fundos
		cor_amarela = (196, 196, 196)
		cor_vermelho = (240, 0, 0)

		#Variaveis para inserir textos na tela
		txt_credito = font.render(str(credito), 1, cor_amarela)
		txt_premio = font.render(str(premio), 1, cor_amarela)
		txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)
		txt_pago = font_pago.render(str(pago), 1, cor_amarela)
		txt_valor_aposta = font.render(str(valor_aposta * linha), 1, cor_amarela)
		txt_sem_credito = font_pago.render('', 1, cor_amarela)

		#imagens da roleta
		imagens = {'01': '01.jpg', '02': '02.jpg', '03': '03.jpg', '04': '04.jpg', '05': '05.jpg',
										   '06': '06.jpg', '07': '07.jpg', '08': '08.jpg', '09': '09.jpg', '10': '10.jpg'}
		retencao = ['01', '02', '03', '04', '05', '06', '07', '08', '09', '10']

		#Variavel que comtem imagens de cada rodda da roleta
		#selecao = self.roleta(0,0)
		#imagens pra roleta

		r1 = pygame.image.load(local + '01.jpg')
		r2 = pygame.image.load(local + '02.jpg')
		r3 = pygame.image.load(local + '03.jpg')

		r4 = pygame.image.load(local + '04.jpg')
		r5 = pygame.image.load(local + '05.jpg')
		r6 = pygame.image.load(local + '06.jpg')

		r7 = pygame.image.load(local + '07.jpg')
		r8 = pygame.image.load(local + '08.jpg')
		r9 = pygame.image.load(local + '09.jpg')

		r10 = pygame.image.load(local + '10.jpg')
		r11 = pygame.image.load(local + '10.jpg')
		r12 = pygame.image.load(local + '10.jpg')

		r13 = pygame.image.load(local + '02.jpg')
		r14 = pygame.image.load(local + '02.jpg')
		r15 = pygame.image.load(local + '02.jpg')

		tela_jogo= True
		tela_config = False
		tela_roda = False
		tela_premio = False
		b_bruxa = False
		b_sinuca = False
		b_redbull = False
		tela_pagamento = False
		tela_premiacao = False
		md = False

		gira = 0
		tm = 0
		lop = 0
		while True:

			if tela_jogo:
				for event in pygame.event.get():
					# Captura evento QUIT que fexa o programa no X
					if event.type == pygame.QUIT:
						pygame.quit()  # Fexa o programa ao paertar no X
						sys.exit()  # Finaliza processos ao fexar o programa

					# Captura evento do mouse
					if event.type == pygame.MOUSEBUTTONDOWN:
						print(pygame.mouse.get_pos())
						tela_jogo = False
						tela_config = True


					# Capttura eventos do teclado
					if event.type == pygame.KEYDOWN:
						if event.key == K_q:  # Informa qual é o evento
							pygame.quit()
							sys.exit()

						# Eventos de inserir nota
						if event.key == 49:
							credito += 200
							txt_credito = font.render(str(credito), 1, cor_amarela)
							self.salva(aposta, premio, pago, credito, valor_aposta)
							self.deposito(200)							
						if event.key == 50:
							credito += 500
							txt_credito = font.render(str(credito), 1, cor_amarela)
							self.salva(aposta, premio, pago, credito, valor_aposta)
							self.deposito(500)							
						if event.key == 51:
							credito += 1000
							txt_credito = font.render(str(credito), 1, cor_amarela)
							self.salva(aposta, premio, pago, credito, valor_aposta)
							self.deposito(1000)							
						if event.key == 52:
							credito += 2000
							txt_credito = font.render(str(credito), 1, cor_amarela)
							self.salva(aposta, premio, pago, credito, valor_aposta)
							self.deposito(2000)							
						if event.key == 53:
							credito += 5000
							txt_credito = font.render(str(credito), 1, cor_amarela)
							self.salva(aposta, premio, pago, credito, valor_aposta)
							self.deposito(5000)
							

						# So meche na maquina com credito
						if credito != 0:
							#Eventos de mudar valor da aposta
							if event.key == 119 or 101 or 114 or 116 or 121:
								if event.key == 119:
									valor_aposta = self.linha(1)
									linha = self.linha(1)
									valor_aposta = linha * aposta
									txt_valor_aposta = font.render(str(linha * aposta), 1, cor_amarela)
									#txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)

								if event.key == 101:
									linha = self.linha(5)
									valor_aposta = linha * aposta
									txt_valor_aposta = font.render(str(linha * aposta), 1, cor_amarela)
									#txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)

								if event.key == 114:
									linha = self.linha(9)
									valor_aposta = linha * aposta
									txt_valor_aposta = font.render(str(linha * aposta), 1, cor_amarela)
									#txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)

								if event.key == 116:
									linha = self.linha(15)
									valor_aposta = linha * aposta
									txt_valor_aposta = font.render(str(linha * aposta), 1, cor_amarela)
									#txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)


								if event.key == 121:
									linha = self.linha(25)
									valor_aposta = linha * aposta
									txt_valor_aposta = font.render(str(linha * aposta), 1, cor_amarela)
									#txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)


							if aposta <= 40:
								# Evento de inserir aposta

								# Aumenta Aposta
								if event.key == pygame.K_a:
									if aposta <= 39:
										aposta += 1
										valor_aposta = linha * aposta
										txt_valor_aposta = font.render(str(valor_aposta), 1, cor_amarela)
										txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)

								#Diminui aposta
								if event.key == pygame.K_s:
									if aposta != 1:
										aposta -= 1
										valor_aposta = linha * aposta
										txt_valor_aposta = font.render(str(valor_aposta), 1, cor_amarela)
										txt_aposta = font_pago.render(str(aposta), 1, cor_amarela)

								#Gira roleta
								if event.key == pygame.K_g:
									if valor_aposta <= credito:
										txt_sem_credito = font_pago.render('', 1, cor_vermelho)

										credito -= valor_aposta
										premio = 0
										pago = 0

										txt_credito = font.render(str(credito), 1, cor_amarela)
										txt_premio = font.render(str(0), 1, cor_amarela)
										txt_pago = font_pago.render(str(0), 1, cor_amarela)
										divi = self.roleta(aposta, linha, valor_aposta)
										selecao = divi[0]
										if divi[1] != None:
											premiacao = divi[1][0]
										self.salva(aposta, premio, pago, credito, valor_aposta)
										tela_jogo = False
										tela_roda = True
									else:
										txt_sem_credito = font_pago.render('Não há créditos suficiente', 1, cor_vermelho)

					# Regula a frequencia de atualização da tela "Frame"
					frame.tick(60)

					# Imprime BackGrounds
					tela.blit(pygame.transform.scale(back_ground, scala), (0, 0))  # Deixa Scalavel

					# imprime Textos
					tela.blit(txt_credito, (335, 55))  # Imprime na tela OBS sempre apos os bcg pois imprime sequencial
					tela.blit(txt_aposta, (1024, 115))
					tela.blit(txt_premio, (704, 55))
					tela.blit(txt_pago, (215, 122))
					tela.blit(txt_valor_aposta, (1073, 55))
					tela.blit(txt_sem_credito, (531, 114))
					#imprime imagens roleta
					tela.blit(r1, (123, 146))
					tela.blit(r2, (123, 292))
					tela.blit(r3, (123, 439))

					tela.blit(r4, (345, 146))
					tela.blit(r5, (345, 292))
					tela.blit(r6, (345, 439))

					tela.blit(r7, (569, 146))
					tela.blit(r8, (569, 292))
					tela.blit(r9, (569, 439))

					tela.blit(r10, (793, 146))
					tela.blit(r11, (793, 292))
					tela.blit(r12, (793, 439))

					tela.blit(r13, (1016, 146))
					tela.blit(r14, (1016, 292))
					tela.blit(r15, (1016, 439))

					# Ativa SCLE de tela
					pygame.display.flip()

					# Serve para atualizar a tela a cada loop
					pygame.display.update()


			#Tela que imprime a rodada
			elif tela_roda:

				if gira < 15:
					r1 = pygame.image.load(local + imagens[random.choice(retencao)])
					r2 = pygame.image.load(local + imagens[random.choice(retencao)])
					r3 = pygame.image.load(local + imagens[random.choice(retencao)])

				if gira < 20:
					r4 = pygame.image.load(local + imagens[random.choice(retencao)])
					r5 = pygame.image.load(local + imagens[random.choice(retencao)])
					r6 = pygame.image.load(local + imagens[random.choice(retencao)])

				if gira < 25:
					r7 = pygame.image.load(local + imagens[random.choice(retencao)])
					r8 = pygame.image.load(local + imagens[random.choice(retencao)])
					r9 = pygame.image.load(local + imagens[random.choice(retencao)])

				if gira < 30:
					r10 = pygame.image.load(local + imagens[random.choice(retencao)])
					r11 = pygame.image.load(local + imagens[random.choice(retencao)])
					r12 = pygame.image.load(local + imagens[random.choice(retencao)])

				if gira < 35:
					r13 = pygame.image.load(local + imagens[random.choice(retencao)])
					r14 = pygame.image.load(local + imagens[random.choice(retencao)])
					r15 = pygame.image.load(local + imagens[random.choice(retencao)])



				if gira == 15:
					r1 = pygame.image.load(local + selecao[0])
					r2 = pygame.image.load(local + selecao[1])
					r3 = pygame.image.load(local + selecao[2])

				if gira == 20:
					r4 = pygame.image.load(local + selecao[3])
					r5 = pygame.image.load(local + selecao[4])
					r6 = pygame.image.load(local + selecao[5])

				if gira == 25:
					r7 = pygame.image.load(local + selecao[6])
					r8 = pygame.image.load(local + selecao[7])
					r9 = pygame.image.load(local + selecao[8])

				if gira == 30:
					r10 = pygame.image.load(local + selecao[9])
					r11 = pygame.image.load(local + selecao[10])
					r12 = pygame.image.load(local + selecao[11])

				if gira == 35:
					gira = 0
					r13 = pygame.image.load(local + selecao[12])
					r14 = pygame.image.load(local + selecao[13])
					r15 = pygame.image.load(local + selecao[14])

					if premiacao == {}:
						tela_jogo = True
						tela_roda = False

					if premiacao != {}:
						tela_premiacao = True
						for pr in premiacao.keys():
							premio += premiacao[pr]

						if divi[1][1] != '':
							premiacao = {}
							print(premio)
							txt_premio = font.render(str(premio), 1, cor_amarela)

							if divi[1][1] == 'bruxa':
								md = True
								b_bruxa = True
								tela_roda = False

							if divi[1][1] == 'sinuca':
								md = True
								b_sinuca = True
								tela_roda = False

							if divi[1][1] == 'redbull':
								md = True
								print('foi premiado')
								b_redbull = True
								tela_roda = False

						else:
							pago = premio
							credito += premio
							premiacao = {}

							txt_credito = font.render(str(credito), 1, cor_amarela)
							txt_premio = font.render(str(premio), 1, cor_amarela)
							txt_pago = font_pago.render(str(pago), 1, cor_amarela)

							tela_premiacao = True
							tela_roda = False

				gira += 1

				frame.tick(60)
				#Imprime na tela
				tela.blit(r1, (123, 146))
				tela.blit(r2, (123, 292))
				tela.blit(r3, (123, 439))

				tela.blit(r4, (345, 146))
				tela.blit(r5, (345, 292))
				tela.blit(r6, (345, 439))

				tela.blit(r7, (569, 146))
				tela.blit(r8, (569, 292))
				tela.blit(r9, (569, 439))

				tela.blit(r10, (793, 146))
				tela.blit(r11, (793, 292))
				tela.blit(r12, (793, 439))

				tela.blit(r13, (1016, 146))
				tela.blit(r14, (1016, 292))
				tela.blit(r15, (1016, 439))

				pygame.display.flip()
				pygame.display.update()
				if md:
					pygame.time.wait(2000)
					md = False

			elif tela_premiacao:
				if tm < 10:
					im_convert = {'01.jpg': '01p.jpg', '02.jpg': '02p.jpg', '03.jpg': '03p.jpg', '04.jpg': '04p.jpg', '05.jpg': '05p.jpg',
					'06.jpg': '06p.jpg', '07.jpg': '07p.jpg', '08.jpg': '08p.jpg', '09.jpg': '09p.jpg', '10.jpg': '10p.jpg' }

					vlrs = self.pr_valor
					for pr in vlrs.keys():
						if pr == 'l1':
							r1 = pygame.image.load(local + im_convert[vlrs[pr]])
							r2 = pygame.image.load(local + im_convert[vlrs[pr]])
							r3 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l4':
							r4 = pygame.image.load(local + im_convert[vlrs[pr]])
							r5 = pygame.image.load(local + im_convert[vlrs[pr]])
							r6 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l7':
							r7 = pygame.image.load(local + im_convert[vlrs[pr]])
							r8 = pygame.image.load(local + im_convert[vlrs[pr]])
							r9 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l10':
							r10 = pygame.image.load(local + im_convert[vlrs[pr]])
							r11 = pygame.image.load(local + im_convert[vlrs[pr]])
							r12 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l15':
							r13 = pygame.image.load(local + im_convert[vlrs[pr]])
							r14 = pygame.image.load(local + im_convert[vlrs[pr]])
							r15 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l65':
							r1 = pygame.image.load(local + im_convert[vlrs[pr]])
							r4 = pygame.image.load(local + im_convert[vlrs[pr]])
							r7 = pygame.image.load(local + im_convert[vlrs[pr]])
							r10 = pygame.image.load(local + im_convert[vlrs[pr]])
							r13 = pygame.image.load(local + im_convert[vlrs[pr]])
						if pr == 'l64':
							r1 = pygame.image.load(local + im_convert[vlrs[pr]])
							r4 = pygame.image.load(local + im_convert[vlrs[pr]])
							r7 = pygame.image.load(local + im_convert[vlrs[pr]])
							r10 = pygame.image.load(local + im_convert[vlrs[pr]])
						if pr == 'l63':
							r1 = pygame.image.load(local + im_convert[vlrs[pr]])
							r4 = pygame.image.load(local + im_convert[vlrs[pr]])
							r7 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l75':
							r7 = pygame.image.load(local + im_convert[vlrs[pr]])
							r10 = pygame.image.load(local + im_convert[vlrs[pr]])
							r13 = pygame.image.load(local + im_convert[vlrs[pr]])
							r4 = pygame.image.load(local + im_convert[vlrs[pr]])
							r1 = pygame.image.load(local + im_convert[vlrs[pr]])
						if pr == 'l74':
							r7 = pygame.image.load(local + im_convert[vlrs[pr]])
							r10 = pygame.image.load(local + im_convert[vlrs[pr]])
							r13 = pygame.image.load(local + im_convert[vlrs[pr]])
							r4 = pygame.image.load(local + im_convert[vlrs[pr]])
						if pr == 'l73':
							r7 = pygame.image.load(local + im_convert[vlrs[pr]])
							r10 = pygame.image.load(local + im_convert[vlrs[pr]])
							r13 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l225':
							r2 = pygame.image.load(local + im_convert[vlrs[pr]])
							r5 = pygame.image.load(local + im_convert[vlrs[pr]])
							r8 = pygame.image.load(local + im_convert[vlrs[pr]])
							r11 = pygame.image.load(local + im_convert[vlrs[pr]])
							r14 = pygame.image.load(local + im_convert[vlrs[pr]])
						if pr == 'l224':
							r2 = pygame.image.load(local + im_convert[vlrs[pr]])
							r5 = pygame.image.load(local + im_convert[vlrs[pr]])
							r8 = pygame.image.load(local + im_convert[vlrs[pr]])
							r11 = pygame.image.load(local + im_convert[vlrs[pr]])
						if pr == 'l23':
							r2 = pygame.image.load(local + im_convert[vlrs[pr]])
							r5 = pygame.image.load(local + im_convert[vlrs[pr]])
							r8 = pygame.image.load(local + im_convert[vlrs[pr]])

						if pr == 'l85':
							r8 = pygame.image.load(local + im_convert[vlrs[pr]])
							r11 = pygame.image.load(local + im_convert[vlrs[pr]])
							r14 = pygame.image.load(local + im_convert[vlrs[pr]])
							r5 = pygame.image.load(local + im_convert[vlrs[pr]])
							r2 = pygame.image.load(local + im_convert[vlrs[pr]])
						if pr == 'l84':
							r8 = pygame.image.load(local + im_convert[vlrs[pr]])
							r11 = pygame.image.load(local + im_convert[vlrs[pr]])
