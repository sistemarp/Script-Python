	def linha5(self, rolos, val):
		imagens = {'01.jpg': 500,'02.jpg': 300, '03.jpg': 25, '04.jpg': 20, '05.jpg': 3, '06.jpg': 2, '07.jpg': 1,
										   '08.jpg': 1, '09.jpg': 1, '10.jpg': 1 }

		valor_ganho = (2 * val / 100)
		bonus = ''
		wins = {}
		img_wins = {}
		#Premio vertical por coluna
		if rolos['l1'] == rolos['l2']:
			if rolos['l1'] == rolos['l3']:
				img_wins['l1'] = rolos['l1']
				valor_ganho += 3 * imagens[rolos['l1']]
				wins['l1'] = valor_ganho
				if rolos['l1'] == '09.jpg':
					bonus = 'bruxa'
				elif rolos['l1'] == '08.jpg':
					bonus = 'sinuca'
				elif rolos['l1'] == '05.jpg':
					bonus = 'redbull'

		if rolos['l4'] == rolos['l5']:
